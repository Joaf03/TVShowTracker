{
  "name": "tvshowtracker",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "migrate-create": "npx node-pg-migrate create -m src/database/migrations",
    "migrate-up": "npx node-pg-migrate up --migrations-dir src/database/migrations",
    "migrate-down": "npx node-pg-migrate down --migrations-dir src/database/migrations",
    "migrate-redo": "npx node-pg-migrate redo --migrations-dir src/database/migrations"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.0",
    "@types/pg": "^8.15.5",
    "node-pg-migrate": "^8.0.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "pg": "^8.16.3"
  }
}
